#!/usr/bin/env python3
from tkinter import *
import webbrowser


def main():
    root = Tk()
    frame = Frame(root)
    frame.pack()

    url = "https://sangwon.users.earthengine.app/view/seaicephenology"

    def OpenUrl(url):
        webbrowser.open_new(url)

    button = Button(root, text="CLCK", command=lambda aurl=url: OpenUrl(aurl))

    button.pack()
    root.mainloop()


if __name__ == "__main__":
    main()
